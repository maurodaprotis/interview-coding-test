{
  "name": "test-software-engineer",
  "version": "1.0.0",
  "description": "ComparaOnline Software Engineer Test",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:ts": "tsc -p ./tsconfig.json --noEmit",
    "test": "jest",
    "build": "rimraf ./build && tsc",
    "after-30-days": "node scripts/products-after-30-days.js"
