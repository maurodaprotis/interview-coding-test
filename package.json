  "repository": {
    "type": "git",
    "url": "git+https://github.com/maurodaprotis/interview-coding-test.git"
  },
  "keywords": [
    "comparaonline",
    "test",
    "software",
    "engineer"
  ],
  "author": "Mauro Daprotis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maurodaprotis/interview-coding-test/issues"
  },
  "homepage": "https://github.com/maurodaprotis/interview-coding-test#readme",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.11.0",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "lint-staged": "^10.4.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
